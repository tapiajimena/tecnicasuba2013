<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
xmlns:util="http://www.springframework.org/schema/util"
xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">



<!-- CADENA SUCURSAL UNO -->

	<bean id="SucursalConCadenaUno"
		class="fiuba.tecnicas.modelo.general.Sucursal" >
		<property name="next"><ref bean="AEslabon_SucursalConCadenaUno"/></property>
		
	</bean>
	
		<bean id="AEslabon_SucursalConCadenaUno"
			class="fiuba.tecnicas.modelo.concreto.Oferta10porcientoFarmacia" >
			<property name="next"><ref bean="BEslabon_SucursalConCadenaUno"/></property>
			
		</bean>
	
			<bean id="BEslabon_SucursalConCadenaUno"
				class="fiuba.tecnicas.modelo.concreto.Oferta2x1" >
				<property name="next"><ref bean="CEslabon_SucursalConCadenaUno"/></property>
				<property name="caracteristicas">
				<util:list list-class="java.util.ArrayList" id="Oferta2x1Caracteristicas" 
					value-type="fiuba.tecnicas.modelo.general.Caracteristica">
					<bean class="fiuba.tecnicas.modelo.general.Caracteristica" >
						<property name="tipoCaracteristica" value="PRODUCTO_DESCRIPCION" />
						<property name="valor" value="Coca" />
					</bean>
				</util:list></property>
			</bean>
			<bean id="CEslabon_SucursalConCadenaUno"
					class="fiuba.tecnicas.modelo.concreto.Oferta10porcientoTarjeta" >
					<property name="next"><ref bean="DEslabon_SucursalConCadenaUno"/></property>
				</bean>
				
					<bean id="DEslabon_SucursalConCadenaUno"
						class="fiuba.tecnicas.modelo.concreto.Oferta10porcientoTarjetaJueves" >
					</bean>

<!-- FIN SUCURSAL UNO -->

<!-- CADENA SUCURSAL CUATRO -->

	<bean id="SucursalConCadenaCuatro"
		class="fiuba.tecnicas.modelo.general.Sucursal" >
		<property name="next"><ref bean="AEslabon_SucursalConCadenaCuatro"/></property>
		
	</bean>
	
		<bean id="AEslabon_SucursalConCadenaCuatro"
			class="fiuba.tecnicas.modelo.concreto.Oferta2x1" >
			<property name="next"><ref bean="BEslabon_SucursalConCadenaCuatro"/></property>
		</bean>
	
			<bean id="BEslabon_SucursalConCadenaCuatro"
				class="fiuba.tecnicas.modelo.concreto.Oferta10porcientoTarjetaJueves" >
				
			</bean>
			
<!-- FIN SUCURSAL CUATRO -->
</beans>